{
  "name": "@carbon/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "node ./dist/index.js",
    "build": "bun build src/index.ts --outdir dist --target node --packages external --sourcemap=linked --format esm",
    "prisma:generate": "bunx prisma generate",
    "prisma:migrate": "bunx prisma migrate dev --name init",
    "seed": "bun prisma/seed.ts",
    "seed:admin": "bun run scripts/seed_admin.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest --run"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "express": "^4.18.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/bcryptjs": "^2.4.4",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0"
  }
}
